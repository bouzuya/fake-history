{
  "name": "fake-history",
  "description": "A History using JavaScript object",
  "version": "0.1.1",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/fake-history/issues"
  },
  "devDependencies": {
    "@types/node": "7.0.12",
    "@types/power-assert": "1.4.29",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-preset-es2015": "6.24.0",
    "beater": "^6.0.1",
    "cpx": "1.5.0",
    "espower-loader": "1.2.0",
    "npm-run-all": "4.0.2",
    "power-assert": "1.4.2",
    "proxyquire": "1.7.11",
    "rimraf": "2.6.1",
    "typescript": "^3.5.3",
    "watch": "1.0.2"
  },
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "homepage": "https://github.com/bouzuya/fake-history",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bouzuya/fake-history.git"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:ts' 'build:es2015' 'build:lib'",
    "build:es2015": "babel .tmp/es2015/ -d .tmp/es5/",
    "build:lib": "npm-run-all -p 'build:lib:*'",
    "build:lib:dts": "cpx '.tmp/es2015/src/*.d.ts' .",
    "build:lib:js": "cpx '.tmp/es5/src/*.js' .",
    "build:ts": "tsc",
    "clean": "rimraf *.js *.d.ts .tmp public",
    "prepublish": "npm-run-all -s 'clean' 'build'",
    "start": "node .tmp/es2015/src/server/index.js",
    "test": "node .tmp/es5/test/index.js",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:es2015": "npm run 'build:es2015' -- --watch",
    "watch:test": "watch --wait 2 'npm test' '.tmp/es5/'",
    "watch:ts": "npm run 'build:ts' -- --watch"
  }
}
